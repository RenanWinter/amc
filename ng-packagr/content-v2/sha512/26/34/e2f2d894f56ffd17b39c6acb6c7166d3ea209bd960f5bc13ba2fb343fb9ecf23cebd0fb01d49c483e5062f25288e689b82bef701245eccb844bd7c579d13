            return this.leftValue;\n        }\n        if (this.isRight) {\n            return this.rightValue;\n        }\n        throw new Error('unexpected error when returning a result of an Either class');\n    }\n}\n;\n\nclass Left extends Either {\n    constructor(value) {\n        super();\n        this._value = value;\n    }\n    get leftValue() {\n        return this._value;\n    }\n    get rightValue() {\n        throw new Error(\"Left intance has no right value\");\n    }\n    get isLeft() {\n        return true;\n    }\n    get isRight() {\n        return false;\n    }\n}\n\nclass Right extends Either {\n    constructor(value) {\n        super();\n        this._value = value;\n    }\n    get leftValue() {\n        throw new Error(\"Right intance has no Left value\");\n    }\n    get rightValue() {\n        let lastValue = this._value;\n        if (this._map && lastValue) {\n            lastValue = this._map(lastValue);\n        }\n        return lastValue;\n    }\n    get isLeft() {\n        return false;\n    }\n    get isRight() {\n        return true;\n    }\n}\n\nclass AppError extends Error {\n    constructor(message, code) {\n        super(message);\n        this.message = message;\n        this.code = code;\n    }\n}\n\nclass CoreClass {\n    constructor() {\n        this.subs = [];\n    }\n    ngOnInit() {\n        this.onInit();\n    }\n    ngOnDestroy() {\n        this.clearAllSubscriptions();\n        this.onDestroy();\n    }\n    ngOnChanges(changes) {\n        this.onChanges(changes);\n    }\n    clearAllSubscriptions() {\n        this.subs.forEach(s => s.unsubscribe());\n    }\n    set subscribe(action) {\n        this.subs.push(action);\n    }\n    onInit() { }\n    onChanges(changes) { }\n    onDestroy() { }\n}\nCoreClass.ɵfac = i0.ɵɵngDeclareFactory({ minVersion: \"12.0.0\", version: \"13.3.6\", ngImport: i0, type: CoreClass, deps: [], target: i0.ɵɵFactoryTarget.Component });\nCoreClass.ɵcmp = i0.ɵɵngDeclareComponent({ minVersion: \"12